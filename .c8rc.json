{
  "all": true,
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "generated/**",
    "scripts/**",
    "test/**",
    "**/*.test.ts",
    "**/*.test.mjs",
    "**/*.spec.ts",
    "dist/**",
    "node_modules/**"
  ],
  "reporter": [
    "text",
    "lcov"
  ],
  "check-coverage": false,
  "lines": 95,
  "functions": 95,
  "branches": 95,
  "statements": 95
}
